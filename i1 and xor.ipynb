   "source": [
    "string = input(\"Enter :\")\n",
    "res_and=\"\"\n",
    "res_xor = \"\"\n",
    "\n",
    "for char in string:\n",
    "    and_val = ord(char)&127\n",
    "    xor_val = ord(char)^127\n",
    "    res_and += chr(and_val)\n",
    "    res_xor += chr(xor_val)\n",
    "    \n",
    "print(\"AND Result: \",res_and)\n",
    "print(\"XOR Result: \",res_xor)\n"
   ]
  }
